{
  "name": "src",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "compile": "npm run generate && tsc",
    "postinstall": "npm run compile",
    "clear": "rm -rf dist",
    "start": "node ./dist/src/index.js",
    "migrate:latest": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:latest",
    "migrate:rollback": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:rollback",
    "migrate:make": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:make"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/server": "^4.10.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.8.0",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "knex": "^3.1.0",
    "node-cache": "^5.1.2",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "sqlite3": "^5.1.7",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@types/node": "^22.0.2",
    "@types/node-cache": "^4.2.5",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
